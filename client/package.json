{
	"name": "vscode-yaml-validation",
	"description": "YAML language server client.",
	"license": "MIT",
	"version": "0.2.0",
	"publisher": "djabraham",
  "aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
	"repository": {
		"url": "https://github.com/djabraham/vscode-yaml-validation.git"
	},
	"engines": {
		"vscode": "^1.6.0"
	},
	"categories": [
		"Languages"
	],
	"activationEvents": [
		"onLanguage:yaml"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "YAML validation configuration",
			"properties": {
				"yaml.maxValidationIssues": {
					"type": "number",
					"default": 100,
					"description": "Controls the maximum number of problems produced by the server."
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"vscode:prepublish": "cd ../server && npm run compile && cd ../client && tsc -p ./",
		"compile": "tsc -p ./"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"dependencies": {
    "vscode-extension-telemetry": "^0.0.7",
 		"vscode-languageclient": "^3.0.2-beta.3",
    "vscode-nls": "^2.0.2"
	}
}